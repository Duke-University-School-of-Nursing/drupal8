{
    "name": "my/project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "require": {
        "php": "^5.5.9|>=7.0.8",
        "composer/installers": "^1.2.0",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5.4",
        "grasmash/drupal-security-warning": "^1.0.0",
        "oomphinc/composer-installers-extender": "^1.1",
        "wikimedia/composer-merge-plugin": "^1.4.1",
        "drupal/core-vendor-hardening": "^8.8",
        "drupal/token": "^1.6.0",
        "drupal/devel": "^2.1.0",
        "drupal/views_bootstrap": "^3.1.0",
        "drupal/entity_embed": "^1.0.0",
        "drupal/filefield_paths": "1.x-dev",
        "drupal/menu_block": "^1.5.0",
        "drupal/insert_view_adv": "^1.0.0-alpha8",
        "drupal/bootstrap_paragraphs": "^2.0.0-beta6",
        "drupal/mailsystem": "^4.2.0",
        "drupal/context": "^4.0.0-beta2",
        "drupal/ckeditor_templates": "^1.1.0",
        "drupal/ctools": "^3.2.0",
        "drupal/embed": "^1.3.0",
        "drupal/field_group": "^3.0.0",
        "drupal/google_analytics": "^2.4.0",
        "drupal/ckeditor_templates_ui": "^1.2.0",
        "drupal/search_api": "^1.15.0",
        "drupal/ckeditor_codemirror": "^2.2.0",
        "drupal/vbo_export": "^3.1.0",
        "drupal/editor_file": "^1.4.0",
        "drupal/crop": "^2.0.0",
        "drupal/shib_auth": "1.x-dev",
        "drupal/entityqueue": "^1.0.0-beta5",
        "drupal/views_bulk_edit": "^2.4.0",
        "drupal/viewsreference": "^1.4.0",
        "drupal/pathauto": "^1.6.0",
        "drupal/entity_browser": "^2.4.0",
        "drupal/admin_toolbar": "^2.0.0",
        "drupal/metatag": "^1.11.0",
        "drupal/url_embed": "1.x-dev",
        "drupal/smtp": "^1.0.0-beta7",
        "drupal/simple_sitemap": "^3.5.0",
        "drupal/mimemail": "^1.0.0-alpha2",
        "drupal/entity_clone": "^1.0.0-beta3",
        "drupal/redirect": "^1.4.0",
        "drupal/adminimal_admin_toolbar": "^1.10.0",
        "drupal/link_attributes": "^1.9.0",
        "drupal/editor_advanced_link": "^1.6.0",
        "drupal/ckeditor_bootstrap_buttons": "^1.2.0",
        "drupal/imce": "1.x-dev",
        "drupal/image_widget_crop": "^2.2.0",
        "drupal/ultimenu": "^2.0.0",
        "drupal/media_directories": "1.x-dev",
        "drupal/entity_reference_revisions": "^1.7.0",
        "drupal/views_bulk_operations": "^3.6.0",
        "drupal/paragraphs": "^1.11.0",
        "drupal/ckeditor_bootstrap_grid": "^1.2.0",
        "drupal/block_class": "^1.1.0",
        "drupal/editor_advanced_image": "^1.0.0-beta5",
        "drupal/webform": "^5.8.0",
        "drupal/video_embed_field": "^2.2.0",
        "drupal/inline_entity_form": "^1.0.0-rc2",
        "drupal/adminimal_theme": "^1.5.0",
        "drupal/bootstrap_barrio": "^4.22.0",
        "drupal/core": "^8.8.2"
    },
    "require-dev": {},
    "config": {
        "sort-packages": true,
        "discard-changes": true,
        "optimize-autoloader": true
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "extra": {
        "enable-patching": true,
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": [],
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ],
            "core": [
                "type:drupal-core"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ]
        },
        "merge-plugin": {
            "include": [
                "modules/custom/*/composer.json"
            ],
            "replace": false,
            "ignore-duplicates": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}